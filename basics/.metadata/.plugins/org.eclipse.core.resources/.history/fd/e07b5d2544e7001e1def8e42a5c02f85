#ifndef INC_FLASH_DRIVER_H_
#define INC_FLASH_DRIVER_H_

#include "stm32f4xx.h"
#include <stdint.h>

struct FLASH_RegDef_t
{
	uint32_t FLASH_ACR;
	uint32_t FLASH_KEYR;
	uint32_t FLASH_OPTKEYR;
	uint32_t FLASH_SR;
	uint32_t FLASH_CR;
	uint32_t FLASH_OPTCR;
};



#endif /* INC_FLASH_DRIVER_H_ */
