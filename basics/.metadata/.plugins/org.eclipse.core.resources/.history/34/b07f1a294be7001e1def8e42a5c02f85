void flash_mass_erase(void)
{

}

void flash_unlock(void)
{
	struct FLASH_RegDef_t *FLASH = (struct FLASH_RegDef_t *)FLASH_BASE_ADDR;

	FLASH->FLASH_KEYR = 0x45670123;
	FLASH->FLASH_KEYR |= (0xCDEF89AB << 16);

	return;
}

