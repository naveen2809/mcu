#ifndef INC_OS_UTILITIES_H_
#define INC_OS_UTILITIES_H_

#include <stdint.h>
#include <string.h>
#include <stdio.h>

typedef struct
{
	uint32_t member;
	QueueNode *prev;
	QueueNode *next;
}QueueNode;

typedef struct
{
	uint32_t length;
	QueueNode *first;
	QueueNode *last;
}Queue;

void os_queue_init(Queue *q);
void os_enqueue(Queue *q,uint32_t value);
uint32_t os_dequeue(Queue *q);

#endif /* INC_OS_UTILITIES_H_ */
