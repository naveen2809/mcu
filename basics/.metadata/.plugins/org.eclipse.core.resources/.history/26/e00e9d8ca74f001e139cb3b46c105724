.syntax unified
.cpu cortex-m4
.fpu softvfp
.thumb

.global _start

.section .text
_start:
	LDR R0,=my_data
	LDRB R4,[R0],#1
	LDRB R5,[R0],#1
	LDRB R6,[R0],#1
	LDRB R7,[R0],#1
	LDRB R8,[R0],#1
Loop:
	B Loop

.section .data
my_data:
	.ascii	"Hello"
